<script type="text/javascript" src="console/js/libs/excanvas.min.js"></script>
<script type="text/javascript" src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
<script type="text/javascript" src="console/js/libs/respond.min.js"></script>
<![endif]-->
<!-- Bootstrap plugins -->
<script src="console/js/bootstrap/bootstrap.js"></script>
<!-- Core plugins ( not remove ) -->
<script src="console/js/libs/modernizr.custom.js"></script>
<!-- Handle responsive view functions -->
<script src="console/js/jRespond.min.js"></script>
<!-- Custom scroll for sidebars,tables and etc. -->
<script src="console/plugins/core/slimscroll/jquery.slimscroll.min.js"></script>
<script src="console/plugins/core/slimscroll/jquery.slimscroll.horizontal.min.js"></script>
<!-- Remove click delay in touch -->
<script src="console/plugins/core/fastclick/fastclick.js"></script>
<!-- Increase jquery animation speed -->
<script src="console/plugins/core/velocity/jquery.velocity.min.js"></script>
<!-- Quick search plugin (fast search for many widgets) -->
<script src="console/plugins/core/quicksearch/jquery.quicksearch.js"></script>
<!-- Bootbox fast bootstrap modals -->
<script src="console/plugins/ui/bootbox/bootbox.js"></script>
<!-- Other plugins ( load only nessesary plugins for every page) -->
<script src="console/plugins/forms/checkall/jquery.checkAll.js"></script>
<script src="console/plugins/charts/sparklines/jquery.sparkline.js"></script>
<script src="console/plugins/ui/waypoint/waypoints.js"></script>
<script src="console/js/jquery.dynamic.js"></script>
<script src="console/js/main.js"></script>
<script src="console/js/pages/tables-basic.js"></script>
<script>

    $('#btn-signup').click(function(event) {
            var formData = {
                firstname : $('input[name=firstname]').val(),
                lastname : $('input[name=lastname]').val(),
                username : $('input[name=username]').val(),
                password : $('input[name=password]').val()
            };

            $.ajax({
               type : 'POST',
                url : '/user',
                data : formData,
                encode : true,
                success : function() {
                    alert("Account Created!");
                }
            });
            event.preventDefault();
    });

    $('#propertyowner').change(function() {
        var checked = $(this).is(':checked');
        if(checked) {
            $('#box-license').removeAttr('checked').attr('disabled','disabled');
            $('#license_num').hide();
        } else {
            $('#box-license').removeAttr('disabled');
        }

    });
</script>
</body>
</html>